<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/dash.css">
    <title>Snapwave</title>
    <link rel="stylesheet" href="style.css">
    <link rel="apple-touch-icon" sizes="180x180" href="favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="favicon/favicon-16x16.png">
    <link rel="manifest" href="favicon/site.webmanifest">
    <link rel="mask-icon" href="favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:ital,wght@0,200..800;1,200..800&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" />
    <!-- Include SweetAlert CSS file -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css">

    <!-- Include SweetAlert JavaScript file -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>

<body>
    <!-- Pop up -->
    <div class="popup" id="loginSuccessModal">
        <div class="popup-content">
            <span class="popup_close" onclick="closepopupModal()">&times;</span>
            <p>Login successful!</p>
        </div>
    </div>
    <nav>
        <div>
            <img src="images/logo.png" alt="Logo">
        </div>
        <div class="searchbarcon">
            <div class="search_con">
                <input id="searchInput" type="text" placeholder="search" autocomplete="off">
                <img class="doc1" src="images/search.png" alt="" srcset=""> 
            </div>
            <div id="userListContainer" class="hid"></div>
        </div>
        <div class="profile_con">
            <div class="profile">
                <img src="<%= user.profilePhoto %>" alt="" srcset="">
            </div>
            <p>
                <%= user.username %>
            </p>
            <div class="dropdown">
                <div class="dropdown-content">
                  <a href="/profile">Profile</a>
                  <a href="/logout">Logout</a>
                </div>
              </div>
        </div>
    </nav>
    <hr>

    <div class="con">
        <div id="videoContainer"></div>
        <div id="openModalBtn" class="uploadbtn">
            +
        </div>
    </div>

    <div class="bottom-navbar">
        <a  href="dashboard"><img class="doc" src="images/dashboard.png" alt=""></a>
        <a href="searchbar"><img class="doc" src="images/search.png" alt=""></a>
        <a href="newpost"><img class="doc" src="images/upload.png" alt=""></a>
        <div class="profile_con1">
            <div class="profile1">
                <img src="<%= user.profilePhoto %>" alt="" srcset="">
            </div>
            
        </div>
      </div>
     







</body>
<script src="/js/dash.js"></script>

</html>